<h1>@user.username</h1>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @user.image %>
  </li>

  <li>
    <strong>Bio:</strong>
    <%= @user.bio %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.account_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.account_path(@conn, :index) %></span>
