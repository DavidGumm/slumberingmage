<h2>Login Page</h2>
<%= form_for @changeset, @action, fn f -> %>
<fieldset>
  <%= label f, :username, for: "user_username" %>
  <%= text_input f, :username %>
  <%= error_tag f, :username %>
  <%= label f, :password, for: "user_password" %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>
  <%= submit "Submit" %>
</fieldset>
<% end %>
